{
  "name": "@surajmandalcell/react-firebase-chat",
  "version": "1.4.2",
  "description": "Actively maintained, community-driven Firebase BaaS for chat applications with an optional chat UI.",
  "homepage": "https://flyer.chat",
  "repository": {
    "type": "git",
    "url": "https://github.com/surajmandalcell/react-firebase-chat.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Suraj Mandal",
  "contributors": [
    "Oleksandr Demchenko <alexdemchenko@yahoo.com>",
    "Vitalii Danylov <vitaliidanylov1992@gmail.com>",
    "Volodymyr Smolianinov <voidozzer@gmail.com>"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "chat",
    "firebase",
    "ios",
    "android",
    "typescript"
  ],
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "compile": "rm -rf lib && tsc -p .",
